<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tab3_weather_container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >
    <RelativeLayout
            android:id="@+id/tab3_weatherfc_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:paddingStart="20dp"
            android:paddingTop="20dp"
            tools:showln="@layout/fragment_tab3"
            android:visibility="gone">

            <RelativeLayout
                android:id="@+id/tab3_interval0_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                >

                <TextView
                    android:id="@+id/tab3_interval0_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="temperature"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval0_temp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval0_time"
                    android:text="WEATHER_STATUS"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval0_weather_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval0_temp"
                    android:text="address"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval0_weather_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval0_weather_description"
                    android:text="address"
                    android:textSize="10sp" />
                <TextView
                    android:id="@+id/tab3_interval0_pop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval0_weather_icon"
                    android:text="address"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/tab3_interval1_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:visibility="visible"
                android:layout_toRightOf = "@id/tab3_interval0_container">
                <TextView
                    android:id="@+id/tab3_interval1_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="temperature"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval1_temp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval1_time"
                    android:text="WEATHER_STATUS"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval1_weather_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval1_temp"
                    android:text="address"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval1_weather_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval1_weather_description"
                    android:text="address"
                    android:textSize="10sp" />
                <TextView
                    android:id="@+id/tab3_interval1_pop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval1_weather_icon"
                    android:text="address"
                    android:textSize="10sp" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/tab3_interval2_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:visibility="visible"
                android:layout_toRightOf = "@id/tab3_interval1_container">
                >

                <TextView
                    android:id="@+id/tab3_interval2_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="temperature"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval2_temp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval2_time"
                    android:text="WEATHER_STATUS"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval2_weather_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval2_temp"
                    android:text="address"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval2_weather_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval2_weather_description"
                    android:text="address"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_interval2_pop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_interval2_weather_icon"
                    android:text="address"
                    android:textSize="10sp" />
            </RelativeLayout>





            <RelativeLayout
            android:id="@+id/tab3_weathercur_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_below = "@id/tab3_interval0_container">
            >

                <TextView
                    android:id="@+id/tab3_weathercur_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="temperature"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_weathercur_temp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_weathercur_time"
                    android:text="WEATHER_STATUS"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_weathercur_weather_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_weathercur_temp"
                    android:text="address"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tab3_weathercur_weather_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tab3_weathercur_weather_description"
                    android:text="address"
                    android:textSize="10sp" />

            </RelativeLayout>

    </RelativeLayout>

    <TextView
            android:id="@+id/tab3_error"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:text="error"
            android:visibility="gone"/>

</FrameLayout>